:@template"ÿA/**
 * @file
 * Styles are organized using the SMACSS technique. @see http://smacss.com/book/
 *
 * When you turn on CSS aggregation at admin/config/development/performance, all
 * of these @include files will be combined into a single file.
 */

/* Import Sass mixins, variables, Compass modules, etc. */
@import "init";

/* HTML element (SMACSS base) rules */
@import "normalize";

/* Layout rules */
@import "layouts/responsive";

/* Component (SMACSS module) rules */
@import "components/misc";
@import "components/variables";
// Optionally, add your own components here.
@import "print";

/* SMACSS theme rules */
/* @import "theme-A"; */
/* @import "theme-B"; */

body {
  font-family: "Roboto";
  color: #444444;
}

a {
	&:link,&:visited {
	  color: #444444;
	  border-bottom: 1px solid #cacaca;
	  text-decoration: none;
	  -webkit-transition: border .15s ease-in-out;
	  -moz-transition: border .15s ease-in-out;
	  -ms-transition: border .15s ease-in-out;
	  -o-transition: border .15s ease-in-out;
	  transition: border .15s ease-in-out;          
	}
  &:focus {
    outline: none;
  }
  &:hover {
    border-bottom: 1px solid #444444;
  }

}

h1 {
  text-transform: uppercase;
  font-size: 2.25em;
  font-weight: 500;
  text-align: center;
  text-shadow: 1px 1px 1px white;
}

/* Homepage Carrousel */
.view-carrousel {
  position: relative;
  ul.flex-direction-nav {
    margin: 0;
    padding: 0;
    width: 100%;
    position: absolute;
    top: 166px;
    z-index: 200;
    li {
      list-style: none;
      margin: 0;
      padding: 0;
      height: 39px;
      a {
		    position: absolute;
		    top: 0;
	      background: url(../images/sprite.png) no-repeat;
        display: block;
        text-indent: -999em;
	      width: 48px;
        height: 39px;
        border: none;
        &.flex-prev {
          left: 10px;
        }
        &.flex-next {
          background-position: -48px 0;
          right: 10px;
        }

      }
    }
  }
  ul.slides {
    margin: 0;
    padding: 0;
    height: 370px;
    overflow: hidden;
    li.slide {
      list-style: none;
      margin: 0;
      padding: 0;
      height: 370px;
      position: relative;
      overflow: hidden;
      .image {
        position: absolute;
        left: 0;
        top: 0;
        z-index: 10;
        height: 370px;
        width: 100%;
        img {
          min-width: 100%;
          min-height: 370px;
          display: block;
        }
      }
      .text {
        max-width: 1180px;
        margin-left: auto;
        margin-right: auto;
        height: 370px;
        position: relative;
        z-index: 100;
	      .title {
	        text-align: center;
	        position: absolute;
	        top: 20px;
	        width: 100%;
	        color: white;
	        text-shadow: 1px 1px 1px black;
	        font-family: "dayposterblackregular";
	        font-size: 4em;
	        line-height: 1em;
	        text-transform: uppercase;
	      }
	      .subtitle {
	        text-align: left;
	        position: absolute;
	        max-width: 80%;
	        bottom: 15px;
	        width: 100%;
	        font-size: 1.625em;
	        line-height: 1.3;
	        font-weight: 700;
	        text-shadow: 1px 1px 1px black;
	        color: white;
	      }
      }
    }
  }
}

/* HEADER */

/* Logo */
#logo {
  border-bottom: none;
}

/* Search form */
#block-views-exp-display-products-page {
  position: absolute;
  top: 20px;
  right: 120px;
  .views-exposed-form {
	  position: relative;
	  .views-exposed-widget {
	    margin: 0;
	    padding: 0;
	    .form-text {
	      border: 1px solid #ededed;
	      font-family: Kameron;
	      padding: 3px 25px 3px 10px;
	      color: #cacaca;
	    }
	  }
    .views-submit-button {
	    position: absolute;
	    right: 3px;
	    top: 3px;
      margin: 0;
      padding: 0;
		  input[type=submit] {
		    margin: 0;
		    padding: 0;
		    background: url(../images/sprite.png) no-repeat -100px 0;
		    width: 20px;
		    height: 20px;
		    border: none;
		    text-indent: -999em;
		  }
		}
	}
}

/* Facebook link */
.block-fb {
  position: absolute;
  top: 7px;
  right: 0;
  a.fb {
    border: none;
    background: url(../images/sprite.png) no-repeat 0 -161px;
    width: 47px;
    height: 47px;
    display: block;
    text-indent: -999em;
    &:hover {
      background-position: -46px -161px;
    }
  }
}

/* Cart link */
.block-cart {
  position: absolute;
  top: 7px;
  right: 60px;
  a.cart {
    border: none;
    background: url(../images/sprite.png) no-repeat 0 -207px;
    width: 47px;
    height: 47px;
    display: block;
    &:hover {
      background-position: -46px -207px;
    }
    span {
      position: relative;
      width: 25px;
      height: 25px;
      display: block;
      text-align: center;
      top: -30px;
      left: 30px;
      background: #444444;
      color: white;
      border-radius: 15px;
      -moz-border-radius: 15px;
      -webkit-border-radius: 15px;
      font-weight: 700;
      font-size: 0.875em;
    }
  }
}

/* Main Menu */
#block-megamenu-main-menu {
  ul#megamenu-main-menu {
    margin: 0;
    padding: 5px;
    background: #dadada;
    border: 1px solid #cacaca;
    li.megamenu-parent {
      margin-right: 70px;
      position: relative;
      z-index: 150;
      a {
        color: white;
        &:hover {
          color: #444444;
        }
        border-bottom: none;
      }
      &.active {
        a {
          color: #444444;
        }
      }
      &.first {
        margin-left: 70px;
      }
      &.last {
        margin-right: 0;
      }
      ul.megamenu-slots-stacking {
        border: none;
        border-radius: 0;
        -moz-border-radius: 0;
        -webkit-border-radius: 0;
        z-index: 100;
        background: $mainRed;
        box-shadow: 2px 2px 4px #888888;
        li.megamenu-slot {
          list-style: none;
          border: none;
          h3.megamenu-slot-title {
            background: none;
            text-align: left;
            a {
              background: none;
	            text-align: left;
	            font-size: 0.875em;
	            font-weight: 300;
	            text-transform: none;
	            color: white;
	            &:hover {
	              text-decoration: none;
	              border-bottom: 1px solid white;
	            }
            }
          }
          ul.megamenu-items {
            margin-left: 20px;
            li.megamenu-item {
              a {
                color: $mainRedLight;
                font-size: 0.8125em;
                font-weight: 300;
                &:hover {
                  text-decoration: none;
                  border-bottom: 1px solid $mainRedLight;
                }
              }
            }
          }
        }
      }
    }
  }
}

/* Secondary Menu */
#block-menu-secondary-navigation {
  position: absolute;
  bottom: 0;
  right: 0;
  ul {
    margin: 0;
    padding: 0;
    li {
      margin: 0;
      padding: 0;
      list-style: none;
      display: inline-block;
      margin-right: 20px;
      &.last {
        margin-right: 0;
      }
      a {
        color: black;
        font-size: 1.125em;
        font-weight: 300;
        padding-bottom: 6px;
        border-bottom: none;
		    -webkit-transition: none;
		    -moz-transition: none;
		    -ms-transition: none;
		    -o-transition: none;
		    transition: none;
        &:hover,&.active {
          border-bottom: 5px solid #444444;
			    -webkit-transition: none;
			    -moz-transition: none;
			    -ms-transition: none;
			    -o-transition: none;
			    transition: none;
        }
      }
    }
  }
}

/* Homepage text */
.catalog-header {
  p {
    font-size: 1.25em;
    color: #444444;
    font-weight: 100;
    text-shadow: 1px 1px 2px white;
    text-align: center;
  }
  a {
    color: #444444;
    border-bottom: 1px solid #cacaca;
    -webkit-transition: border .15s ease-in-out;
    -moz-transition: border .15s ease-in-out;
    -ms-transition: border .15s ease-in-out;
    -o-transition: border .15s ease-in-out;
    transition: border .15s ease-in-out;          
    &:hover {
      border-bottom-color: #444444;
	    -webkit-transition: border .15s ease-in-out;
	    -moz-transition: border .15s ease-in-out;
	    -ms-transition: border .15s ease-in-out;
	    -o-transition: border .15s ease-in-out;
	    transition: border .15s ease-in-out;          
    }
  }
}

/* Homepage categories */
.catalog-blocks-front {
  ul.blocks {
    margin: 0;
    padding: 0;
    li.block {
      margin: 0;
      padding: 0;
      list-style: none;
      float: left;
      width: 32%;
      margin-right: 2%;
      margin-bottom: 20px;
      height: 240px;
      position: relative;
      &.last {
        margin-right: 0;
      }
      a {
        display: block;
        height: 240px;
        color: white;
        -webkit-transition: opacity .15s ease-in-out;
        -moz-transition: opacity .15s ease-in-out;
        -ms-transition: opacity .15s ease-in-out;
        -o-transition: opacity .15s ease-in-out;
        transition: opacity .15s ease-in-out;          
        border: none;
        &:hover {
          opacity: 0.6;
	        -webkit-transition: opacity .15s ease-in-out;
	        -moz-transition: opacity .15s ease-in-out;
	        -ms-transition: opacity .15s ease-in-out;
	        -o-transition: opacity .15s ease-in-out;
	        transition: opacity .15s ease-in-out;          
	        border: none;
        }
        .category {
          position: absolute;
          bottom: 20px;
          right: 10px;
          font-size: 3em;
          font-weight: 400;
        }
      }
    }
  }
}

/* Product catalogus */
.view-display-products, .view-collection-products {
  ul.products {
    margin: 0;
    padding: 0;
    li.product {
      width: 25%;
      float: left;
      height: 315px;
      border-right: 1px dotted #d8d8d8;
      margin-bottom: 20px;
      list-style: none;
      &.last {
        border-right: none;
        width: 24%;
      }
      .node {
        text-align: center;
        position: relative;
        margin: 10px;
        height: 295px;
        h2.node-title {
          position: absolute;
          bottom: 70px;
          text-align: center;
          width: 100%;
          font-size: 1em;
          line-height: 1.2;
          a {
            border: none;
          }
        }
        .field-type-image {
          text-align: center;
          a {
            border: none;
          }
          img {
            border: 2px solid #e3e3e4;
          }
        }
        .go-to-product {
          position: absolute;
          bottom: 10px;
          left: 10%;
          width: 80%;
          a {
            border: none;
            text-align: right;
            display: block;
            margin: 0 auto;
            padding: 5px 10px 5px 50px;
            color: white;
            background: $mainRed url(../images/sprite.png) no-repeat 0 -42px;
		        -webkit-transition: all .15s ease-in-out;
		        -moz-transition: all .15s ease-in-out;
		        -ms-transition: all .15s ease-in-out;
		        -o-transition: all .15s ease-in-out;
		        transition: all .15s ease-in-out;          
            &:hover {
              background-color: #e88b82;
              border-bottom: none;
			        -webkit-transition: all .15s ease-in-out;
			        -moz-transition: all .15s ease-in-out;
			        -ms-transition: all .15s ease-in-out;
			        -o-transition: all .15s ease-in-out;
			        transition: all .15s ease-in-out;          
            }
          }
        }
      }
    }
  }
}

.not-front {
  .view-display-products {
    ul.products {
      li.product {
        height: 340px;
        .node {
          height: 320px;
          h2.node-title {
            bottom: 95px;
          }
          .product-price {
            position: absolute;
            bottom: 60px;
            width: 100%;
            text-align: center;
            color: #a6a6a6;
            font-weight: 700;
          }
        }
      }
    }
  }
}

/* Product Display detail page */
.node-type-product-display {
  .field-field-images {
    #wrap {
      border: 3px solid white;
      margin-bottom: 20px;
      z-index: auto !important;
      a {
        border-bottom: none;
      }
    }
    .cloud-zoom-gallery-thumbs {
      a {
        margin: 0;
        padding: 0;
        margin-right: 10px;
        border: 2px solid white;
        display: inline-block;
        img {
          display: block;
        }
      }
    }
  }
  
  .call-to-action {
    margin-top: 20px;
    background: #f2f2f2;
    border: 1px solid #cacaca;
    padding: 10px;
    .price {
      font-size: 1.375em;
      color: #888888;
    }
    form {
      margin: 20px 0;
      .form-item-quantity {
        margin: 0;
        padding: 0;
        float: left;
        margin-right: 20px;
        label {
          display: inline;
        }
      }
      .form-submit {
        color: white;
        font-weight: 700;
        text-transform: lowercase;
        border: none;
        cursor: pointer;
        background: url(../images/sprite.png) $mainRed no-repeat left -127px;
        padding: 6px 10px 6px 40px;
        position: relative;
        top: -3px;
      }
      &.out-of-stock {
        .form-item-quantity {
          display: none;
        }
        .form-submit {
          cursor: default;
          background: #ededed;
          padding-left: 10px;
          color: #adadad;
        }
      }
    }
  }
}

/* Login pagina */
form#user-login {
  #user_login_form {
    border-right: 1px solid #cacaca;
    margin-bottom: 40px;
  }
  .create-account {
  }
}

/* Campaign Monitor */
#block-campaign-monitor-subscribe-campaign-monitor-subscribe {
  margin: 0;
  padding: 0;
  text-align: center;
  max-width: 1180px;
  margin-left: auto;
  margin-right: auto;
  height: 75px;
  line-height: 75px;
  .label {
    float: left;
    color: white;
    font-size: 1.25em;
    font-weight: 300;
    padding-left: 100px;
    margin-right: 30px;
  }
  form {
    .form-item {
      margin: 0;
      padding: 0;
      float: left;
      margin-right: 10px;
      .form-text {
        border: none;
        height: 35px;
        font-family: Kameron;
        color: #cacaca;
        width: 300px;
        padding-left: 10px;
      }
    }
	  .form-submit {
	    background: url(../images/sprite.png) no-repeat 0 -85px;
	    float: left;
	    margin-top: 21px;
	    width: 40px;
	    height: 35px;
	    text-indent: -1000em;
	    border: none;
	  }
	}
}

/* Add to cart message */
div.commerce-add-to-cart-confirmation {
  width: 650px !important;
  margin-left: -30% !important;
  padding-bottom: 20px !important;
  a {
    border-bottom: none;
  }
}

/* Cart */
.view-commerce-cart-form {
  margin-bottom: 20px;
  table {
    width: 100%;
    tr {
      td {
        padding-top: 20px;
        padding-bottom: 20px;
        .field-name-field-images {
          float: left;
          margin-right: 20px;
        }
        h2.field-name-title-field {
          margin: 0;
          padding: 0;
          font-size: 1em;
          color: #444444;
          font-weight: 500;
        }
        a.goto-product {
          font-weight: 100;
          font-size: 0.875em;
        }
        &.views-field-nothing {
          padding-left: 20px;
        }
        input.delete-line-item {
          border: none;
          cursor: pointer;
          width: 47px;
          height: 47px;
          background: url(../images/sprite.png) no-repeat left -255px;
          text-indent: -999em;
          &:hover {
            background-position: -47px -255px;
          }
        }
      }
      &.even {
        background: #fafafa;
      }
    }
  }
}


/* Footer */
#footer-wrapper {
  background: white;
  #footer {
	  margin: 0;
	  padding: 0;
	  max-width: 1180px;
	  margin-left: auto;
	  margin-right: auto;
	  font-weight: 300;
	  .column-2 {
	    h2 {
	      text-transform: uppercase;
	      font-size: 1.125em;
        font-weight: 500;
        margin-top: 16px;
	    }
	    ul {
	      margin: 0;
	      padding: 0;
	      li {
	        margin: 0;
	        padding: 0;
	        list-style: none;
	        float: left;
	        width: 50%;
	      }
	    }
	  }
	  .column-3 {
	    text-align: right;
	  }
	  a {
	    color: #888888;
	    border-bottom: 1px solid #888888;
      -webkit-transition: all .15s ease-in-out;
      -moz-transition: all .15s ease-in-out;
      -ms-transition: all .15s ease-in-out;
      -o-transition: all .15s ease-in-out;
      transition: all .15s ease-in-out;          
	    &:hover {
	      color: #444444;
	      border-bottom: 1px solid #888888;
        -webkit-transition: all .15s ease-in-out;
        -moz-transition: all .15s ease-in-out;
        -ms-transition: all .15s ease-in-out;
        -o-transition: all .15s ease-in-out;
        transition: all .15s ease-in-out;          
	    }
	  }
	}
